<li>
	{{name}}

	{{#if devoured}}
	<!-- data-id {{id}} is coming from the ID of the burger, {{devoured}} is the supposed boolean of 0 or 1 -->
	<button class="change-devoured" data-id="{{id}}" data-newDevoured="{{devoured}}">CHOW DOWN</button>
	
		{{else}}
	
	<button class="delete-burger" data-id="{{this.id}}">KILL</button>
	{{/if}}
</li>
